{
  "private": true,
  "name": "ianwalter",
  "version": "0.0.1",
  "scripts": {
    "gen.readme": "node scripts/generateReadme",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "mr test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/ianwalter.git"
  },
  "author": {
    "name": "Ian Walter",
    "url": "https://ianwalter.dev"
  },
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ianwalter/ianwalter/issues"
  },
  "homepage": "https://github.com/ianwalter/ianwalter#readme",
  "devDependencies": {
    "@changesets/cli": "^2.18.0",
    "@generates/mister": "^0.0.4",
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "@ianwalter/renovate-config": "workspace:*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "renovate": {
    "extends": [
      "@ianwalter"
    ],
    "ignorePaths": [
      "archived/*"
    ]
  }
}
